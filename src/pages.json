{
  "easycom": {
    "autoscan": true,
    "custom": {
      // uni-ui 规则如下配置
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
      "^u-(.*)": "vk-uview-ui/components/u-$1/u-$1.vue"
    }
  },
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/scanning/scanning",
      "style": {
        "navigationBarTitleText": "识别",
        "navigationBarBackgroundColor": "#f8f8f8"
      }
    },
    {
      "path": "pages/riceResult/riceResult",
      "style": {
        "navigationBarTitleText": "种子识别"
      }
    },
    {
      "path": "pages/virusResult/virusResult",
      "style": {
        "navigationBarTitleText": "病害识别"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    // "navigationBarBackgroundColor": "#F8F8F8",
    "navigationBarBackgroundColor": "#fff",
    "backgroundColor": "#F8F8F8"
  }
}
